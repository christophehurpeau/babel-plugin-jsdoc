{
  "name": "babel-plugin-add-jsdoc-annotations",
  "version": "5.1.0",
  "description": "Adds JSDoc annotations to source code",
  "repository": "christophehurpeau/babel-plugin-jsdoc",
  "keywords": [
    "babel-plugin",
    "jsdoc",
    "flow",
    "annotations"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "lint": "eslint src/",
    "preversion": "npm run lint && npm run build && npm run test",
    "release": "pob-repository-check-clean && pob-release",
    "test": "mocha -b -c -u tdd --es_staging test/index.js",
    "test:watch": "mocha -w -b -c -u tdd --es_staging test/index.js",
    "version": "pob-version",
    "watch": "babel --watch src --out-dir dist"
  },
  "dependencies": {
    "better-log": "^1.3.3",
    "doctrine": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.7.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^2.0.0",
    "mocha": "^3.1.0",
    "pob-release": "^2.1.2"
  },
  "bugs": {
    "url": "https://github.com/christophehurpeau/babel-plugin-jsdoc/issues"
  },
  "homepage": "https://github.com/christophehurpeau/babel-plugin-jsdoc/tree/babel-plugin-add-jsdoc-annotations",
  "directories": {
    "test": "test"
  },
  "author": "Moti Zilberman (motiz88@gmail.com)"
}
