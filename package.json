{
  "name": "babel-plugin-add-jsdoc-annotations",
  "version": "5.0.0",
  "description": "Adds JSDoc annotations to source code",
  "repository": "motiz88/babel-plugin-add-jsdoc-properties",
  "keywords": [
    "babel-plugin",
    "jsdoc",
    "flow",
    "annotations"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "lint": "eslint src/",
    "preversion": "npm run lint && npm run build",
    "release": "pob-repository-check-clean && pob-release",
    "test": "mocha -b -c -u tdd --es_staging test/index.js",
    "test:watch": "mocha -w -b -c -u tdd --es_staging test/index.js",
    "version": "pob-version",
    "watch": "babel --watch src --out-dir dist"
  },
  "dependencies": {
    "better-log": "^1.3.3",
    "doctrine": "^1.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-node6": "^0.2.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "mocha": "^3.0.0",
    "pob-release": "^2.1.2"
  },
  "bugs": {
    "url": "https://github.com/motiz88/babel-plugin-add-jsdoc-properties/issues"
  },
  "homepage": "https://github.com/motiz88/babel-plugin-add-jsdoc-properties",
  "directories": {
    "test": "test"
  },
  "author": "Moti Zilberman (motiz88@gmail.com)"
}
